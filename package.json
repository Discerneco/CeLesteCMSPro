{
	"name": "celestecms-pro",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate:sqlite",
		"db:migrate": "node --loader tsx src/lib/server/db/migrate.ts",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx src/lib/server/db/seed.ts",
		"db:seed-standalone": "tsx src/lib/server/db/seed-standalone.ts",
		"test:db": "tsx test-db-standalone.ts",
		"db:cli": "tsx db-cli.ts"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250522.0",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@sveltejs/adapter-cloudflare": "^7.0.3",
		"@sveltejs/kit": "^2.21.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.7",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^22.15.18",
		"daisyui": "^5.0.35",
		"drizzle-kit": "^0.31.1",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.7.0",
		"globals": "^16.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.30.1",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.7",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.1",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.12",
		"@libsql/client": "^0.15.6",
		"@lucide/svelte": "^0.511.0",
		"@paralleldrive/cuid2": "^2.2.2",
		"better-auth": "^1.2.8",
		"better-sqlite3": "^11.10.0",
		"dotenv": "^16.5.0",
		"drizzle-orm": "^0.43.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"better-sqlite3",
			"esbuild",
			"sharp",
			"workerd"
		],
		"overrides": {
			"esbuild@<=0.24.2": ">=0.25.0",
			"cookie@<0.7.0": ">=0.7.0"
		}
	}
}
