{
	"name": "celestecms-pro",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate:sqlite",
		"db:migrate": "node --import tsx src/lib/server/db/migrate.ts",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx src/lib/server/db/seed.ts",
		"db:seed-standalone": "tsx src/lib/server/db/seed-standalone.ts",
		"test:db": "tsx test-db-standalone.ts",
		"db:cli": "tsx db-cli.ts",
		"test:email": "tsx test-email.ts"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250712.0",
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.31.0",
		"@sveltejs/adapter-cloudflare": "^7.0.5",
		"@sveltejs/kit": "^2.22.5",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^24.0.13",
		"daisyui": "^5.0.46",
		"drizzle-kit": "^0.31.4",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.10.1",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.35.6",
		"svelte-check": "^4.2.2",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.36.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.2.0",
		"@libsql/client": "^0.15.9",
		"@lucide/svelte": "^0.525.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@paralleldrive/cuid2": "^2.2.2",
		"@tiptap/core": "^3.3.0",
		"@tiptap/extension-image": "^3.3.0",
		"@tiptap/extension-link": "^3.3.0",
		"@tiptap/starter-kit": "^3.3.0",
		"@types/express": "^5.0.3",
		"arctic": "^3.7.0",
		"better-sqlite3": "^12.2.0",
		"dotenv": "^17.2.0",
		"drizzle-orm": "^0.44.2",
		"express": "^5.1.0",
		"mrmime": "^2.0.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"better-sqlite3",
			"esbuild",
			"sharp",
			"workerd"
		],
		"overrides": {
			"esbuild@<=0.24.2": ">=0.25.0",
			"cookie@<0.7.0": ">=0.7.0"
		}
	}
}
